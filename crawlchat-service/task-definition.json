{
  "family": "crawlchat-preprocessor-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::169164939839:role/crawlchat-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::169164939839:role/crawlchat-preprocessor-task-role",
  "containerDefinitions": [
    {
      "name": "preprocessor",
      "image": "169164939839.dkr.ecr.ap-south-1.amazonaws.com/crawlchat-preprocessor:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "ap-south-1"
        },
        {
          "name": "S3_BUCKET",
          "value": "crawlchat-data"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/crawlchat-preprocessor",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
