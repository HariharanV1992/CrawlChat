{
  "QueueConfigurations": [
    {
      "Id": "pdf-upload-notification",
      "QueueArn": "arn:aws:sqs:ap-south-1:169164939839:pdf-preprocess-queue",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "uploaded_documents/"
            },
            {
              "Name": "suffix",
              "Value": ".pdf"
            }
          ]
        }
      }
    }
  ]
}
